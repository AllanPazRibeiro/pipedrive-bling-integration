{
  "name": "link-api-test",
  "version": "1.0.0",
  "description": "This repo is a integration between pipedrive and bling",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "lint:check": "eslint -c .eslintrc --ext .ts src",
    "start": "DEBUG=api-ts-template* node dist/index.js",
    "dev": "DEBUG=api-ts-template* nodemon src/index.ts",
    "test": "jest \"^.+test\\.ts$\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllanPazRibeiro/link-api-test.git"
  },
  "keywords": [
    "pipedrive",
    "bling"
  ],
  "author": "Allan Paz Ribeiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AllanPazRibeiro/link-api-test/issues"
  },
  "homepage": "https://github.com/AllanPazRibeiro/link-api-test#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.1.1",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/xml-js": "^1.0.0",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "js2xmlparser": "^4.0.1",
    "mongodb": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  }
